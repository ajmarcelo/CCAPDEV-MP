<!DOCTYPE html>

<html>
	<head>
		<title>Table of Contents</title>
		<meta name="description" content="Table of Contents Page for Members">
		<meta name="author" content="Andrea Jean Marcelo">

		<link rel = "stylesheet" type = "text/css" href = "/css/contentTable.css">
	</head>

	<body id="TofCBody">
		<!-- Main Tabs -->
		{{#if role}}
			{{> headerM}}
		{{else}}
			{{> headerV}}
		{{/if}}

		<div id="parentTOC">
			<div class="tableofContents">
				<h1 id="TofCtitle"> Table Of Contents </h1>
				<br>
				{{#each plants}}
					{{> plantTOC}}
				{{/each}}					
			</div>	
		</div>
	</body>
</html>
